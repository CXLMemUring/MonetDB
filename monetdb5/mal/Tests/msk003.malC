# The msk vector are intended to represent candidate lists

b := bat.new(:int);
bat.append(b,1);
bat.append(b,3);
bat.append(b,5);
bat.append(b,0);
bat.append(b,1);

bv2:bat[:msk]:= bat.mask(b);
# should produce (1,1,1,1,1)
io.print(bv2);

cnd:bat[:msk] := algebra.select(bv2, nil:bat[:oid], 0, 2, true, true, false);
# should produce (1,0,0,1,1)
io.print(cnd);

prj:bat[:int]:= bat.project(bv2, cnd);
io.print(prj);
